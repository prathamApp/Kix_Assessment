<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.fragment_profile.Fragment_Profile"
    android:background="@color/white"
    android:clickable="true"
    android:focusable="true">

    <TextView
        android:id="@+id/tv_profileName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Profile"
        android:textSize="@dimen/_20sdp"
        android:textStyle="bold"
        android:gravity="center_horizontal"
        android:layout_marginTop="@dimen/_10sdp"/>

    <TextView
        android:id="@+id/tv_studCount"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Student Assigned :"
        android:textSize="@dimen/_15sdp"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_below="@+id/tv_profileName"
        android:layout_marginStart="@dimen/_50sdp"/>

   <TextView
        android:id="@+id/tv_householdCount"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Households :"
        android:textSize="@dimen/_15sdp"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_below="@+id/tv_studCount"
        android:layout_marginStart="@dimen/_50sdp"/>

</RelativeLayout>